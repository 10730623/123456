        "from numpy.linalg import inv\n",
        "import numpy as np\n",
        "  \n",
        "a = np.array([[3,  2,  -1],\n",
        "             [2,  -2,  4],\n",
        "             [-1,  0.5,  -1]])\n",
        "                \n",
        "ainv = inv(np.matrix(a))\n",
        "ainv           \n",
        "b= np.array([1,-2,0])\n",
        "\n",
        "c= np.linalg.solve(a,b)\n",
        "\n",
        "print(ainv)\n",
        "print(b)\n",
        "print(c)\n"
